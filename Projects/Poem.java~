import java.io*;

public class Poem
{
  public static void main(String [] args)
  {
    System.out.println("Hey Diddly Diddle\n\t\tThe mouse and the " +
                       "Fiddle\nThe cow jumped over the moon\n\n\t\tAnd" +
                       " the dish ran away with the spoon.\n\nTheEnd.");
  }
} 